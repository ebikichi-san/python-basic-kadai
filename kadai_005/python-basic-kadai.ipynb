        "# 上辺、下辺、高さを設定\n",
        "top_side = 10\n",
        "bottom_side = 20\n",
        "high = 5\n",
        "\n",
        "# 台形の面積を計算\n",
        "area = (top_side + bottom_side) * high / 2\n",
        "\n",
        "# 結果を表示\n",
        "print(\"台形の面積は\", area, \"c㎡です。\")"
